   "source": [
    "# Uma threadming independente\n",
    "import time\n",
    "def contador(n):\n",
    "    print('T-menos', n)\n",
    "    n -= 1\n",
    "    time.sleep(5)\n",
    "from threading import Thread\n",
    "t = Thread(target = contador, args=(10,))\n",
    "t.start()"
   ]
